,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00

  // Bitmap data for 4stop_Screen...
};
// Arrays of bitmap data for each screen group (1stop and 2stop)
const unsigned char* screenGroupBitmaps[][3] = {
  {image_data_Opening_Screen, NULL}, // Screen group 0 (Opening)
  {image_data_1stop_Screen, NULL}, // Screen group 1 (1stop)
  {image_data_2stop_Screen, NULL}, // Screen group 2 (2stop)
};

int currentScreenGroup = 0; // Variable to keep track of the current screen group
int currentScreenIndex = 0; // Variable to keep track of the current screen index within a group
int buttonPins[] = {A0, A1, A2, A3}; // Array to store the pin numbers for each button


void setup() {
  Serial.begin(9600);

  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;);
  }
  delay(2000); // Added missing semicolon here

 for (int i = 0; i < 4; i++) {
    pinMode(buttonPins[i], INPUT_PULLUP); // Use internal pull-up resistors
  }
}

void loop() {
  // Check for button press to switch screen group
  for (int i = 0; i < 4; i++) {
    if (digitalRead(buttonPins[i]) == HIGH) { // Check for button press
      // Switch to the corresponding screen group
      currentScreenGroup = i;
      currentScreenIndex = 0; // Reset screen index
      break; // Exit loop after processing one button press
    }
  }

  // Clear the display before drawing new content
  display.clearDisplay();

  // Display the current screen for the current screen group
  if (currentScreenGroup >= 0 && currentScreenGroup < sizeof(screenGroupBitmaps) / sizeof(screenGroupBitmaps[0])) {
    // Retrieve the bitmap data for the current screen group and current screen index
    const unsigned char* currentBitmap = screenGroupBitmaps[currentScreenGroup][currentScreenIndex];
    // Display the bitmap if available
    if (currentBitmap != NULL) {
      display.drawBitmap(0, 0, currentBitmap, 128, 64, 1);
    }
  }

  // Update the display with the drawn content
  display.display();

  // Check for button press to switch to the next screen in the group
  if (digitalRead(buttonPins[currentScreenGroup]) == HIGH) {
    // Increment the screen index within the current screen group
    currentScreenIndex++;
    if (currentScreenIndex >= 2) { // Adjust the number based on the number of screens in the group
      currentScreenIndex = 0; // Reset screen index if reached the last screen
    }
    delay(200); // Debounce delay
  }
}
